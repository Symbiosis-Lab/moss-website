{
  "name": "moss",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tauri dev",
    "build": "tauri build",
    "frontend:dev": "vite",
    "frontend:build": "vite build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "vitest run",
    "test:integration": "echo 'No integration tests yet - create vitest.integration.config.js and test files'",
    "test:backend": "cd src-tauri && source $HOME/.cargo/env && cargo test",
    "test:all": "npm run test && npm run test:integration",
    "test:coverage": "vitest run --coverage",
    "generate:type": "cd src-tauri && cargo check"
  },
  "devDependencies": {
    "@tauri-apps/api": "2.8.0",
    "@tauri-apps/cli": "2.8.0",
    "@types/node": "^24.5.0",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "typescript": "^5.9.2",
    "vite": "^5.4.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@tauri-apps/plugin-deep-link": "^2.4.1"
  }
}
